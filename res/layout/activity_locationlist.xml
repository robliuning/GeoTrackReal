<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Locationlist" >
   <RelativeLayout
    	android:layout_width="match_parent"
    	android:layout_height="wrap_content">
    		<TextView
    			android:id="@+id/list_interval"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content" />
    		<TextView
    			android:id="@+id/list_distance"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_toRightOf="@id/list_interval" />
    		<TextView
    			android:id="@+id/list_time_start"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_below="@id/list_interval" />	
    		<TextView
    			android:id="@+id/list_time_end"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_toRightOf="@id/list_time_start"
		      	android:layout_below="@id/list_interval" />	    
	</RelativeLayout>    
	<FrameLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		<ListView
			android:id="@+id/tracker_list"
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
		</ListView>
	</FrameLayout>
    
</RelativeLayout>